<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

     <servlet>
     	<servlet-name>insertServlet</servlet-name>
     	<servlet-class>productPackage.insertServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>insertServlet</servlet-name>
     	<url-pattern>/insertServlet</url-pattern>
     </servlet-mapping>
     
     
     <servlet>
     	<servlet-name>getAllproduct</servlet-name>
     	<servlet-class>productPackage.getAllproduct</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>getAllproduct</servlet-name>
     	<url-pattern>/getAllproduct</url-pattern>
     </servlet-mapping>
     
     
     <servlet>
     	<servlet-name>updateServlet</servlet-name>
     	<servlet-class>productPackage.updateServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>updateServlet</servlet-name>
     	<url-pattern>/updateServlet</url-pattern>
     </servlet-mapping>
     
     
     <servlet>
     	<servlet-name>deleteServlet</servlet-name>
     	<servlet-class>productPackage.deleteServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>deleteServlet</servlet-name>
     	<url-pattern>/deleteServlet</url-pattern>
     </servlet-mapping>
     

     
     
     <servlet>
    	<servlet-name>itemDisplay</servlet-name>
    	<servlet-class>productPackage.itemDisplay</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>itemDisplay</servlet-name>
    	<url-pattern>/itemDisplay</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
    <welcome-file>itemDisplay</welcome-file>
</welcome-file-list>
	
	
	<!-- user system -->
	
	
	 
     <servlet>
    	<servlet-name>userInsert</servlet-name>
    	<servlet-class>sellerPackage.userInsert</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>userInsert</servlet-name>
    	<url-pattern>/userInsert</url-pattern>
	</servlet-mapping>
	
	
	
	 <servlet>
    	<servlet-name>loginServlet</servlet-name>
    	<servlet-class>sellerPackage.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>loginServlet</servlet-name>
    	<url-pattern>/loginServlet</url-pattern>
	</servlet-mapping>
     

</web-app>